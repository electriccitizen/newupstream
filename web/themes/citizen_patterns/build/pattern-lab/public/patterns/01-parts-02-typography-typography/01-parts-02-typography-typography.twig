{% if bannerTitle %}
	<div class="typography-element">
		<div class="element-title">H1 on Image Header</div>
		<div class="layout__region--content banner-title">
			<div class="block-field-banner">
				<img src="{{ bannerTitle.img.src }}" alt="{{ bannerTitle.img.alt }}" />
			</div>
			<div class="block-title">
				<h1>
					{% for listItem in listItems.one %}
						{{ listItem.shortTitle }}
					{% endfor %}
				</h1>
			</div>
		</div>
	</div>
{% endif %}

{% if paragraph %}
	<div class="typography-element">
		<div class="element-title">Paragraph</div>
		<div class="constrain-text">
			<p>
				{% for listItem in listItems.one %}
					{{ listItem.description }}
				{% endfor %}
			</p>
		</div>
	</div>
{% endif %}

