{
  "name": "electriccitizen/newupstream",
  "description": "Install drops-8 with Composer on Pantheon using EC build.",
  "type": "project",
  "license": "MIT",

  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "package",
      "package": {
        "name": "electriccitizen/docksal",
        "version": "1.3.1",
        "type": "drupal-custom-module",
        "source": {
          "url": "https://github.com/electriccitizen/docksal.git",
          "type": "git",
          "reference": "1.3.1"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "d3/d3",
        "version": "v3.5.17",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/d3/d3/archive/v3.5.17.zip",
          "type": "zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "components/highlightjs",
        "version": "9.7.0",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/components/highlightjs/archive/9.7.0.zip",
          "type": "zip"
        }
      }
    }
  ],
    "require": {
    "components/highlightjs": "9.7.*",
    "composer/installers": "^1.6",
    "cweagans/composer-patches": "^1.0",
    "d3/d3": "3.5.*",
    "drupal-composer/drupal-scaffold": "^2.0.1",
    "drupal/addanother": "^1.0@RC",
    "drupal/addtoany": "~1",
    "drupal/admin_toolbar": "~1",
    "drupal/antibot": "^1.2",
    "drupal/auto_entitylabel": "^3.0@beta",
    "drupal/components": "^1.0",
    "drupal/config_devel": "^1.2",
    "drupal/config_direct_save": "^1.0",
    "drupal/config_filter": "^1.4.0",
    "drupal/config_ignore": "^2.1",
    "drupal/config_split": "^1.4",
    "drupal/config_update": "^1.5",
    "drupal/console": "^1.8.0",
    "drupal/console-core": "^1.8.0",
    "drupal/core": "^8.6.3",
    "drupal/crop": "^2.0",
    "drupal/ctools": "^3.0@alpha",
    "drupal/default_content": "^1.0.0@alpha",
    "drupal/devel": "^2.0",
    "drupal/easy_breadcrumb": "^1.6",
    "drupal/entity": "^1.0.0-rc1",
    "drupal/entity_browser": "^2.0.0",
    "drupal/entity_embed": "1.x-dev",
    "drupal/entity_reference_revisions": "^1.6.0",
    "drupal/entity_usage": "^2.0@beta",
    "drupal/extlink": "^1.1",
    "drupal/field_group": "^3.0",
    "drupal/flippy": "1.x-dev",
    "drupal/focal_point": "^1.0",
    "drupal/geolocation": "^1.11",
    "drupal/google_analytics": "^3.0",
    "drupal/hreflang": "^1.1",
    "drupal/image_style_quality": "^1.3",
    "drupal/inline_entity_form": "^1.0@beta",
    "drupal/layout_builder_restrictions": "^2.2",
    "drupal/libraries": "^3.0@alpha",
    "drupal/linkit": "^4.3",
    "drupal/media_entity": "^2.0",
    "drupal/media_entity_actions": "^1.0@alpha",
    "drupal/media_entity_document": "^1.1",
    "drupal/media_entity_generic": "^1.0@alpha",
    "drupal/media_entity_image": "^1.2",
    "drupal/menu_block": "^1.4",
    "drupal/metatag": "^1.0",
    "drupal/paragraphs": "^1.8",
    "drupal/pathauto": "^1.3",
    "drupal/pathologic": "1.x-dev",
    "drupal/quicklink": "^1.1",
    "drupal/redirect": "^1.3",
    "drupal/responsive_preview": "^1.0@beta",
    "drupal/search_api": "^1.10",
    "drupal/stage_file_proxy": "^1.0@alpha",
    "drupal/structure_sync": "^1.14",
    "drupal/superfish": "~1",
    "drupal/token": "^1.5",
    "drupal/twig_tweak": "^2.0",
    "drupal/video_embed_field": "^2.0",
    "drupal/video_embed_media": "^2.0",
    "drupal/views_bulk_operations": "^3.2",
    "drupal/views_infinite_scroll": "^1.5",
    "drupal/webform": "^5.1",
    "drupal/xmlsitemap": "^1.0@alpha",
    "drush/drush": "~8",
    "electriccitizen/docksal": "~1",
    "geerlingguy/drupal-vm": "^4.7",
    "rvtraveller/qs-composer-installer": "^1.1",
    "webflo/drupal-core-strict": "^8",
    "webflo/drupal-finder": "^1.0",
    "wikimedia/composer-merge-plugin": "~1.3"
  },
  "require-dev": {
    "mikey179/vfsstream": "^1.2",
    "behat/behat": "3.*",
    "behat/mink": "^1.7",
    "behat/mink-extension": "^2.2",
    "behat/mink-goutte-driver": "^1.2",
    "jcalderonzumba/gastonjs": "^1.0.2",
    "jcalderonzumba/mink-phantomjs-driver": "^0.3.1",
    "drupal/drupal-extension": "^3.1",
    "drush-ops/behat-drush-endpoint": "^0.0.4",
    "pantheon-systems/quicksilver-pushback": "~1",
    "phpunit/phpunit": "^4.8",
    "symfony/css-selector": "^2.8"
  },
  "conflict": {
      "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ],
    "psr-4": { "Pantheon\\TerminusBuildTools\\": "src" }
  },
  "scripts": {
    "build-assets": [
      "@prepare-for-pantheon",
      "composer install --optimize-autoloader"
    ],
    "drupal-unit-tests": "cd web/core && ../../vendor/bin/phpunit --testsuite=unit --exclude-group Composer,DependencyInjection,PageCache",
    "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
    "prepare-for-pantheon": "DrupalProject\\composer\\ScriptHandler::prepareForPantheon",
    "post-install-cmd": [
      "@drupal-scaffold",
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "post-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "post-create-project-cmd": [
      "@drupal-scaffold",
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ]
  },
  "extra": {
    "installer-paths": {
      "web/core": ["type:drupal-core"],
      "web/modules/contrib/{$name}": ["type:drupal-module"],
      "web/profiles/{$name}": ["type:drupal-profile"],
      "web/libraries/{$name}": ["type:drupal-library"],
      "web/themes/{$name}": ["type:drupal-theme"],
      "drush/contrib/{$name}": ["type:drupal-drush"],
      ".docksal": ["type:drupal-custom-module"]
    },
    "build-env": {
      "install-cms": [
        "drush site-install citizen --account-mail={account-mail} --account-name={account-name} --account-pass={account-pass} --site-mail={site-mail} --site-name={site-name} --yes"
      ],
      "export-configuration": "drush config-export --yes"
    },
    "drupal-scaffold": {
      "source": "https://raw.githubusercontent.com/pantheon-systems/drops-8/{version}/{path}",
      "includes": [
        "sites/default/default.services.pantheon.preproduction.yml",
        "sites/default/settings.pantheon.php"
      ],
      "excludes": [
        ".csslintrc",
        ".editorconfig",
        ".eslintignore",
        ".eslintrc.json",
        "web.config"
      ],
      "terminus": {
        "compatible-version": "^2.0"
      }
    },
    "patches": {
      "drupal/addanother":{
        "URL doesnt match machine name": "https://www.drupal.org/files/issues/addanother-url-doesnt-match-machine-name-2851584-3.patch"
      },
      "drupal/menu_block": {
        "Enable menu block by page functionality": "https://www.drupal.org/files/issues/2018-08-08/menu_block-follow-2756675-55.patch",
        "Enable using active menus as menu block titles": "https://www.drupal.org/files/issues/menu_block-label_configuration-2809699-51.patch",
        "Enable link functionality on menu block titles": "https://www.drupal.org/files/issues/menu_block-title_as_link-1-d8.patch"
      },
      "drupal/entity_embed": {
        "Array to string conversion for Media Image": "https://www.drupal.org/files/issues/2019-09-02/entity-embed-array-to-string--3069448-17.patch"
      },
      "drupal/field_group": {
        "Incompatibility with inline entity form": "https://www.drupal.org/files/issues/2018-10-30/field_group-inline_entity_form_compatibility-2986704-3.patch"
      }
    },
    "enable-patching": true
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
